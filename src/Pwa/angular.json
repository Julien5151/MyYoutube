{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "player-pwa": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": true
        },
        "@schematics/angular:directive": {
          "standalone": true
        },
        "@schematics/angular:pipe": {
          "standalone": true
        }
      },
      "root": "projects/player-pwa",
      "sourceRoot": "projects/player-pwa/src",
      "prefix": "pwa",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/player-pwa",
            "index": "projects/player-pwa/src/index.html",
            "browser": "projects/player-pwa/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/player-pwa/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["projects/player-pwa/src/favicon.ico", "projects/player-pwa/src/assets", "projects/player-pwa/src/manifest.webmanifest"],
            "styles": ["projects/player-pwa/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "projects/player-pwa/ngsw-config.json"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "projects/player-pwa/src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "player-pwa:build:production"
            },
            "development": {
              "buildTarget": "player-pwa:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "player-pwa:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/player-pwa/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": ["projects/player-pwa/src/favicon.ico", "projects/player-pwa/src/assets", "projects/player-pwa/src/manifest.webmanifest"],
            "styles": ["projects/player-pwa/src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/player-pwa/**/*.ts", "projects/player-pwa/**/*.html"]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": ["@angular-eslint/schematics"],
    "analytics": false
  }
}
